    "bcryptjs": "^2.4.3",
    "config": "^3.1.0",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "jest": "^24.9.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "react-material-ui-form-validator": "^2.0.9"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
